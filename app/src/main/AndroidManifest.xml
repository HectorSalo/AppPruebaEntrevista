<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.thefactoryhka.apppruebaentrevista">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.thefactoryhka.apppruebaentrevista.listRecibosModule.ui.ListRecibosActivity"
            android:label="@string/title_activity_lista_recibos"/>
        <activity
            android:name="com.thefactoryhka.apppruebaentrevista.nuevoReciboModule.ui.NuevoReciboActivity"
            android:label="@string/title_activity_nuevo_recibo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".homeModule.LaunchActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".homeModule.MainActivity" />
    </application>

</manifest>